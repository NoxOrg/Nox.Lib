@using Microsoft.AspNetCore.Components.Routing

@inject IProjectConfiguration NoxService

@if(NoxService.Entities is not null)
{

    @foreach (var entity in NoxService.Entities)
    {
        string url = "/ui/" + entity.PluralName; 

        <NoxUiNavLink href="@url" Icon="@Icon.Dataset">
            @entity.PluralName
        </NoxUiNavLink>
    }
}