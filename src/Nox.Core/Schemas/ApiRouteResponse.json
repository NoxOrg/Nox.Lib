{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "NOX Api Route Response",
  "description": "Defines NOX api route response",
  "type": "object",
  "additionalProperties": false,
  "required": ["type"],
  "properties": {
    "type": {
      "type": "string",
      "description": "The type of the route response"
    },
    "isCollection": {
      "type": "boolean",
      "description": "Whether the response returns a collection"
    }
  }
}